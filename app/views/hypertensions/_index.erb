<div id="flash">
      <% flash.each do |key, value| -%>
        <div id="flash_<%= key %>" class="alert alert-<%=key%> d-flex justify-content-center" ><%= value %></div>
      <% end %>
</div>
<div id="hypertension-record">
    <% if @history.any? %>
        <% @history.each do |history|%>
            <div>SBP :<%= history.SBP %></div>
            <div>DBP :<%= history.DBP %></div>
            <fiv>Notes :<%= history.notes %></fiv>
            <div>Medication :<%= history.medication %></div>
            <br\>
        <% end %>
    <% else %>
        <%= content_tag("div","No history of your daily record.")%>
    <% end %>
</div>